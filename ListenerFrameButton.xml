<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\..\FrameXML\UI.xsd">
  <Script file="ListenerFrameButton.lua"/>
  <Button name="ListenerFrameButton" intrinsic="true" enableMouse="true">
    <Size x="15" y="15"/>
    <Layers>
      <Layer level="BACKGROUND">
        <Texture setallpoints="true" parentKey="bg">
          <Color r="1" g="1" b="1" a="0.8"/>
        </Texture>
      </Layer>
    </Layers>
    <Scripts> 
      <OnLoad>
        self.SetOn     = ListenerAddon.FrameButton_SetOn
        self.SetColor  = ListenerAddon.FrameButton_SetColor
        self.MyRefresh = ListenerAddon.FrameButton_Refresh
      </OnLoad>
    </Scripts>
  </Button>
</Ui>